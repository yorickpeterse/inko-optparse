import std.env
import std.test.(Filter, Tests)

import optparse.test_lexer
import test_optparse

class async Main {
  fn async main {
    let tests = Tests.new

    test_lexer.tests(tests)
    test_optparse.tests(tests)

    tests.filter = Filter.from_string(env.arguments.opt(0).unwrap_or(''))
    tests.run
  }
}
